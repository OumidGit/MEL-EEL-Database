) AS src(HP, KW,Eff_50,Eff_75,Eff_100,PF_50,PF_75,PF_100,V,FLA,FLA_125)
ON (tgt.HP = src.HP) AND (tgt.V = src.V)
WHEN MATCHED THEN
  UPDATE SET HP=src.HP, KW=src.KW,Eff_50=src.Eff_50,Eff_75=src.Eff_75,Eff_100=src.Eff_100,PF_50=src.PF_50,PF_75=src.PF_75,PF_100=src.PF_100,V=src.V,FLA=src.FLA,FLA_125=src.FLA_125
WHEN NOT MATCHED THEN
  INSERT(HP, KW,Eff_50,Eff_75,Eff_100,PF_50,PF_75,PF_100,V,FLA,FLA_125) VALUES(src.HP, src.KW,src.Eff_50,src.Eff_75,src.Eff_100,src.PF_50,src.PF_75,src.PF_100,src.V,src.FLA,src.FLA_125);
