USE MEL_EEL_Automation_Dev;
GO

-- Duty reference Table Creation (Define a duty type for each duty possible)
IF OBJECT_ID('ref.motor','U') IS NULL
BEGIN
    CREATE TABLE ref.motor(
        HP DECIMAL(10,3) NOT NULL,
        KW DECIMAL(10,3)  NOT NULL,
        Eff_50 DECIMAL(10,3)  NOT NULL,
        Eff_75 DECIMAL(10,3)  NOT NULL,
        Eff_100 DECIMAL(10,3)  NOT NULL,
        PF_50 DECIMAL(10,3)  NOT NULL,
        PF_75 DECIMAL(10,3)  NOT NULL,
        PF_100 DECIMAL(10,3)  NOT NULL,
        V DECIMAL(10,3)  NOT NULL,
        FLA DECIMAL(10,3) NOT NULL,
        FLA_125 DECIMAL(10,3)  NOT NULL,
        CONSTRAINT PK_motor PRIMARY KEY (HP,V)
    );
END
GO

-- 2) Duty Types Definition
